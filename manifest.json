{
  "manifest_version": 2,

  "name": "GroovyShark",
  "description": "This extension lets you control music played on your desktop browser from your mobiel phone.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://grooveshark.com/*"],
      "js": ["js/grooveshark.js"]
    }
  ],
  "web_accessible_resources": ["js/grooveshark.js"],
  "permissions": [
    "http://grooveshark.com/"
  ]
}
